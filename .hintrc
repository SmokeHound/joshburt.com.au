{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/language": "off",
    "axe/text-alternatives": "off",
    "meta-viewport": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 142",
    "not firefox <= 142",
    "not opera <= 121",
    "not chrome <= 109"
  ]
}